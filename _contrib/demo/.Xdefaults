Xft.rgba: none
Xft.hintstyle: hintfull

URxvt.scrollBar: false
URxvt.transparent: true
URxvt.internalBorder: 16

URxvt.font: xft:RobotoMono:medium:pixelsize=27:antialias=true:hinting=true
URxvt.boldFont: xft:RobotoMono:bold:pixelsize=27:antialias=true:hinting=true

! special
